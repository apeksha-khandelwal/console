{
  "extends": [
    "eslint:recommended",
    "plugin:import/errors",
    "plugin:import/warnings",
    "plugin:react/recommended",
  ],
  "env": {
    "browser": true,
    "es6": true,
  },
  "rules": {
    "brace-style": 2,
    "camelcase": 2,
    "consistent-return": 0,
    "curly": [2, "all"],
    "eol-last": [2, "unix"],
    "eqeqeq": [2, "allow-null"],
    "guard-for-in": 2,
    "import/no-unresolved": ["error", {"ignore": ["./dist/templates"] }],
    "indent": [2, 2, {"SwitchCase": 1, "VariableDeclarator": 2}],
    "jsx-quotes": "error",
    "no-alert": 1,
    "no-comma-dangle": 0,
    "no-console": 1,
    "no-debugger": 1,
    "no-else-return": "error",
    "no-global-strict": 0,
    "no-multi-spaces": [2],
    "no-underscore-dangle": 0,
    "no-unused-vars": ["error", {"varsIgnorePattern": "React"}],
    "no-use-before-define": 1,
    "no-var": 2,
    "prefer-template": 1,
    "quotes": [2, "single"],
    "radix": 2,
    "react/jsx-curly-spacing": 2,
    "react/jsx-equals-spacing": 2,
    "react/jsx-no-duplicate-props": 2,
    "react/jsx-uses-react": "error",
    "react/jsx-uses-vars": "error",
    "react/no-unknown-property": "error",
    "react/prop-types": 0,
    "react/self-closing-comp": ["error", {"component": true, "html": false}],
    "semi": 2,
    "wrap-iife": [2, "inside"],
  },
  "parserOptions": {
    "ecmaFeatures": {
      "jsx": true,
    },
    "ecmaVersion": 6,
    "sourceType": "module",
  },
  "globals": {
    "_": true,
    "ace": true,
    "afterEach": true,
    "beforeEach": true,
    "describe": true,
    "expect": true,
    "inject": true,
    "it": true,
    "module": true,
    "moment": true,
    "require": true,
    "spyOn": true,
  },
  "plugins": [
    "react",
  ],
  "settings": {
    "import/extensions": [".js", ".jsx"],
    "import/resolver": {"node": {"extensions": [".js", ".jsx"]}},
  }
}
